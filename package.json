{
   "name": "grass",
   "version": "0.0.1",
   "private": false,
   "license": "MIT",
   "description": "API nodejs with typescript",
   "devDependencies": {
      "@types/jest": "^26.0.14",
      "@types/node": "^8.0.29",
      "eslint": "^7.9.0",
      "jest": "^26.4.2",
      "nodemon": "^1.18.10",
      "ts-jest": "^26.4.0",
      "ts-node": "3.3.0",
      "tsc-watch": "^4.2.8",
      "tsconfig-paths": "^3.9.0",
      "typescript": "^4.0.3"
   },
   "dependencies": {
      "aws-sdk": "^2.1023.0",
      "bcryptjs": "^2.4.3",
      "commander": "^8.3.0",
      "cors": "^2.8.5",
      "dotenv": "^10.0.0",
      "express": "^4.17.1",
      "express-rate-limit": "^5.5.1",
      "firebase": "^9.3.0",
      "fs-extra": "^10.0.0",
      "google-auth-library": "^7.10.2",
      "googleapis": "^89.0.0",
      "http-status-codes": "^2.1.4",
      "is-pdf": "^1.0.0",
      "is-svg": "^4.3.1",
      "joi": "^17.4.2",
      "jsonwebtoken": "^8.5.1",
      "lodash": "^4.17.21",
      "log4js": "^6.3.0",
      "method-override": "^3.0.0",
      "module-alias": "^2.2.2",
      "multer": "^1.4.3",
      "multer-s3": "^2.10.0",
      "mysql": "^2.18.1",
      "mysql2": "^2.3.3-rc.0",
      "nexmo": "^2.9.1",
      "node-twitter-api": "^1.8.0",
      "nodemailer": "^6.7.0",
      "oauth": "^0.9.15",
      "onesignal-node": "^3.3.0",
      "reflect-metadata": "^0.1.13",
      "request-promise": "^4.2.6",
      "sequelize": "^6.9.0",
      "sequelize-cli": "^6.3.0",
      "socket.io": "^4.3.2",
      "typeorm": "0.2.38"
   },
   "_moduleAliases": {
      "@entity": "build/app/Entity",
      "@repository": "build/app/Repositories",
      "@controller": "build/controller",
      "@service": "build/app/Services",
      "@middleware": "build/app/Middleware",
      "@notification": "build/app/Notifications",
      "@provider": "build/app/Providers",
      "@event": "build/app/Events",
      "@listener": "build/app/Listeners",
      "@schema": "build/app/Schema",
      "@const": "build/app/Constant",
      "@util": "build/app/Utilities"
   },
   "scripts": {
      "start": "tsc && nodemon --watch ./src --exec ts-node -r tsconfig-paths/register ./src/index.ts",
      "dev": "tsc-watch  --onSuccess \"nodemon ./build/index.js\"",
      "prod": "rm -rf build && tsc && nodemon build/index.js",
      "build": "tsc",
      "ts": "nodemon src/index.ts",
      "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "make:migration": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
      "recreatedb": "ts-node ./node_modules/typeorm/cli.js schema:drop && npm run migrate && npm run seed",
      "rollback": "ts-node ./node_modules/typeorm/cli.js migration:revert",
      "command": "ts-node command",
      "seed": "./node_modules/.bin/sequelize db:seed:all",
      "test": "jest --forceExit --detectOpenHandles"
   }
}
